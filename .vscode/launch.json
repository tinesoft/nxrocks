    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Jest Current File",
            "program": "${workspaceFolder}/node_modules/@nrwl/cli/bin/nx.js",
            "cwd": "${workspaceFolder}",
            "args": [
                "test",
                "nx-spring-boot",
                "--testPathPattern=${fileBasenameNoExtension}",
                "--runInBand"
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "neverOpen",
            "disableOptimisticBPs": true
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Je2est Current File",
            "program": "${workspaceFolder}/node_modules/@nrwl/cli/bin/nx.js",
            "cwd": "${workspaceFolder}",
            "args": [
                "e2e",
                "nx-spring-boot-e2e",
                "--testPathPattern=${fileBasenameNoExtension}",
                "--runInBand"
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "neverOpen",
            "disableOptimisticBPs": true
        }
    ]
}