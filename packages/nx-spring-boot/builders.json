{
  "$schema": "../../node_modules/@angular-devkit/architect/src/builders-schema.json",
  "builders": {
    "run": {
      "implementation": "./src/builders/run/builder",
      "schema": "./src/builders/run/schema.json",
      "description": "Builder to run the application"
    },
    "serve": {
      "implementation": "./src/builders/run/builder",
      "schema": "./src/builders/serve/schema.json",
      "description": "Builder to serve the application (alias to 'run' builder)"
    },
    "test": {
      "implementation": "./src/builders/test/builder",
      "schema": "./src/builders/test/schema.json",
      "description": "Builder to test the application"
    },
    "buildJar": {
      "implementation": "./src/builders/build-jar/builder",
      "schema": "./src/builders/build-jar/schema.json",
      "description": "Builder to build the application's executable Jar"
    },
    "buildWar": {
      "implementation": "./src/builders/build-war/builder",
      "schema": "./src/builders/build-war/schema.json",
      "description": "Builder to build the application's executable War"
    },
    "buildImage": {
      "implementation": "./src/builders/build-image/builder",
      "schema": "./src/builders/build-image/schema.json",
      "description": "Builder to build the application's OCI image"
    },
    "buildInfo": {
      "implementation": "./src/builders/build-info/builder",
      "schema": "./src/builders/build-info/schema.json",
      "description": "Builder to build the applications's build information"
    }
  }
}
