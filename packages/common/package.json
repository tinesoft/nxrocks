{
  "name": "@nxrocks/common",
  "description": "Common library to share code among the `@nxrocks/*` plugins.",
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "version": "0.0.0-dev",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./testing": "./testing/index.ts"
  },
  "files": [
    "dist",
    "!**/*.tsbuildinfo"
  ],
  "license": "MIT",
  "author": "Tine Kondo ",
  "repository": {
    "type": "git",
    "url": "https://github.com/tinesoft/nxrocks.git",
    "directory": "packages/common"
  },
  "homepage": "https://github.com/tinesoft/nxrocks/blob/master/packages/common#readme",
  "bugs": {
    "url": "https://github.com/tinesoft/nxrocks/issues"
  },
  "keywords": [
    "nx-flutter",
    "nx-spring-boot",
    "nx-quarkus",
    "nx-micronaut",
    "nx-ktor",
    "nx-melos"
  ],
  "dependencies": {
    "hpagent": "^1.2.0",
    "unzipper": "^0.12.3",
    "fs-extra": "^10.1.0",
    "@nx/devkit": ">=20.0.0",
    "minimatch": "^9.0.3",
    "semver": "^7.6.0",
    "@swc/helpers": "~0.5.11"
  },
  "peerDependencies": {},
  "devDependencies": {}
}
