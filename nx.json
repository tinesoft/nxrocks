{
  "pluginsConfig": {
    "@nx/js": {
      "analyzeSourceFiles": true
    }
  },
  "release": {
    "projects": ["packages/*"],
    "projectsRelationship": "independent",
    "releaseTagPattern": "{projectName}/v{version}",
    "git": {
      "commitMessage": "chore(release): ðŸš€ release packages [skip ci]"
    },
    "changelog": {
      "projectChangelogs": {
        "createRelease": "github"
      }
    },
    "version": {
      "preVersionCommand": "bunx nx run-many -t build",
      "conventionalCommits": true,
      "generatorOptions": {
        "packageRoot": "dist/packages/{projectName}",
        "updateDependents": "auto"
      }
    }
  },
  "tasksRunnerOptions": {
    "default": {
      "options": {
        "runtimeCacheInputs": ["node -v", "echo $RUNNER_OS"],
        "canTrackAnalytics": false,
        "showUsageWarnings": true
      }
    }
  },
  "workspaceLayout": {
    "appsDir": "e2e",
    "libsDir": "packages"
  },
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "test": {
      "options": {
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "coverage": true
        }
      }
    },
    "e2e": {
      "options": {
        "passWithNoTests": true,
        "runInBand": true
      },
      "dependsOn": ["nxrocks:populate-local-registry"],
      "configurations": {
        "ci": {
          "ci": true,
          "coverage": true
        }
      }
    },
    "e2e-ci": {
      "options": {
        "passWithNoTests": true,
        "runInBand": true
      },
      "dependsOn": ["nxrocks:populate-local-registry"],
      "configurations": {
        "ci": {
          "ci": true,
          "coverage": true
        }
      }
    },
    "nx-release-publish": {
      "options": {
        "packageRoot": "dist/packages/{projectName}"
      }
    },
    "@nx/js:tsc": {
      "cache": true,
      "dependsOn": ["^build"],
      "inputs": ["production", "^production"]
    },
    "@nx/eslint:lint": {
      "cache": true,
      "inputs": [
        "default",
        "{workspaceRoot}/.eslintrc.json",
        "{workspaceRoot}/.eslintignore",
        "{workspaceRoot}/eslint.config.js"
      ]
    }
  },
  "nxCloudAccessToken": "ZDRmZDczZWUtMDU2My00YzQ0LTlkMWEtZjM2YjZlZWNmNGIxfHJlYWQ=",
  "parallel": 1,
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "eslint:lint",
        "extensions": ["ts", "tsx", "js", "jsx", "html", "vue"]
      }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "e2e",
        "ciTargetName": "e2e-ci",
        "disableJestRuntime": false
      },
      "include": [
        "e2e/nx-flutter-e2e/**/*",
        "e2e/nx-ktor-e2e/**/*",
        "e2e/nx-melos-e2e/**/*",
        "e2e/nx-micronaut-e2e/**/*",
        "e2e/nx-quarkus-e2e/**/*",
        "e2e/nx-spring-boot-e2e/**/*",
        "e2e/smoke/**/*"
      ]
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "test",
        "ciTargetName": "test-ci",
        "disableJestRuntime": false
      },
      "include": [
        "packages/common/**/*",
        "packages/common-cli/**/*",
        "packages/common-jvm/**/*",
        "packages/create-nx-flutter/**/*",
        "packages/create-nx-ktor/**/*",
        "packages/create-nx-micronaut/**/*",
        "packages/create-nx-quarkus/**/*",
        "packages/create-nx-spring-boot/**/*",
        "packages/nx-flutter/**/*",
        "packages/nx-ktor/**/*",
        "packages/nx-melos/**/*",
        "packages/nx-micronaut/**/*",
        "packages/nx-quarkus/**/*",
        "packages/nx-spring-boot/**/*"
      ]
    },
    {
      "plugin": "@nxrocks/nx-ktor",
      "options": {
        "buildTargetName": "build",
        "installTargetName": "install",
        "testTargetName": "test",
        "cleanTargetName": "clean",
        "formatTargetName": "format",
        "applyFormatTargetName": "apply-format",
        "checkFormatTargetName": "check-format",
        "runTargetName": "run",
        "serveTargetName": "serve",
        "buildImageTargetName": "build-image",
        "publishImageTargetName": "publish-image",
        "publishImageLocallyTargetName": "publish-image-locally",
        "runDockerTargetName": "run-docker"
      }
    },
    {
      "plugin": "@nxrocks/nx-flutter",
      "options": {
        "analyzeTargetName": "analyze",
        "cleanTargetName": "clean",
        "formatTargetName": "format",
        "testTargetName": "test",
        "assembleTargetName": "assemble",
        "attachTargetName": "attach",
        "driveTargetName": "drive",
        "genL10nTargetName": "gen-l10n",
        "installTargetName": "install",
        "runTargetName": "run",
        "buildAarTargetName": "build-aar",
        "buildApkTargetName": "build-apk",
        "buildAppbundleTargetName": "build-appbundle",
        "buildBundleTargetName": "build-bundle",
        "buildIosTargetName": "build-ios",
        "buildIosFrameworkTargetName": "build-ios-framework",
        "buildIpaTargetName": "build-ipa",
        "pubGetTargetName": "pub-get",
        "pubUpgradeTargetName": "pub-upgrade",
        "pubDowngradeTargetName": "pub-downgrade",
        "pubOutdatedTargetName": "pub-outdated",
        "pubAddTargetName": "pub-add",
        "pubRemoveTargetName": "pub-remove",
        "pubRunTargetName": "pub-run",
        "pubPublishTargetName": "pub-publish",
        "pubDepsTargetName": "pub-deps",
        "pubVersionTargetName": "pub-version"
      }
    },
    {
      "plugin": "@nxrocks/nx-spring-boot",
      "options": {
        "buildTargetName": "build",
        "installTargetName": "install",
        "testTargetName": "test",
        "cleanTargetName": "clean",
        "formatTargetName": "format",
        "applyFormatTargetName": "apply-format",
        "checkFormatTargetName": "check-format",
        "runTargetName": "run",
        "serveTargetName": "serve",
        "buildImageTargetName": "build-image",
        "buildInfoTargetName": "build-info"
      }
    }
  ],
  "namedInputs": {
    "default": ["{projectRoot}/**/*", "sharedGlobals"],
    "production": [
      "default",
      "!{projectRoot}/.eslintrc.json",
      "!{projectRoot}/eslint.config.js",
      "!{projectRoot}/**/?(*.)+(spec|test).[jt]s?(x)?(.snap)",
      "!{projectRoot}/tsconfig.spec.json",
      "!{projectRoot}/jest.config.[jt]s",
      "!{projectRoot}/src/test-setup.[jt]s",
      "!{projectRoot}/test-setup.[jt]s"
    ],
    "sharedGlobals": ["{workspaceRoot}/.nx/workflows/**/**"]
  },
  "defaultBase": "develop",
  "cli": {
    "packageManager": "bun"
  },
  "useLegacyCache": false
}
